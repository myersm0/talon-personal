app: /musescore/i
-

octave up:
	key(cmd-up)

octave down:
	key(cmd-down)

staff up:
	key(alt-up)

staff down:
	key(alt-down)

measure right:
	key(cmd-right)

measure left:
	key(cmd-left)

whole note:
	key(7)

half note:
	key(6)

quarter note:
	key(5)

eighth note:
	key(4)

sixteenth note:
	key(3)

